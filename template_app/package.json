{
  "name": "taska",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:clean": "expo start --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "web:prod": "expo start --web --no-dev --minify",
    "build:web": "expo export:web",
    "build:web-optimized": "expo export:web --source-maps false --minify",
    "lint": "expo lint",
    "analyze-bundle": "expo export:web --no-minify && npx webpack-bundle-analyzer web-build/static/js/*.js",
    "preload-fonts": "expo install expo-font",
    "clear-cache": "expo start --clear",
    "reset-cache": "npx expo install --fix && expo start --clear",
    "fix-metro": "rm -rf node_modules/.expo && rm -rf .expo && expo start --clear"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@kichiyaki/react-native-barcode-generator": "^0.6.7",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.3.0",
    "@react-native-picker/picker": "2.11.0",
    "@react-navigation/bottom-tabs": "^7.3.14",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.10",
    "@supabase/supabase-js": "^2.50.0",
    "@types/react-native-modern-datepicker": "^1.0.5",
    "base64-arraybuffer": "^1.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "expo": "53.0.11",
    "expo-blur": "~14.1.5",
    "expo-checkbox": "~4.1.4",
    "expo-clipboard": "~7.1.4",
    "expo-constants": "~17.1.6",
    "expo-document-picker": "^13.1.6",
    "expo-file-system": "^18.1.10",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.3.0",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.5",
    "expo-local-authentication": "~16.0.4",
    "expo-router": "~5.1.0",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.8",
    "expo-web-browser": "~14.1.6",
    "express": "^5.1.0",
    "node-fetch": "^2.7.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.3",
    "react-native-calendars": "^1.1312.1",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-gifted-chat": "^2.4.0",
    "react-native-keyboard-controller": "^1.17.5",
    "react-native-maps": "1.20.1",
    "react-native-modern-datepicker": "^1.0.0-beta.91",
    "react-native-otp-entry": "^1.8.4",
    "react-native-pager-view": "6.7.1",
    "react-native-picker-select": "^9.3.1",
    "react-native-progress": "^5.0.1",
    "react-native-raw-bottom-sheet": "^3.0.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-tab-view": "^4.1.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-virtualized-view": "^1.0.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "expo": {
    "install": {
      "exclude": [
        "react-native-safe-area-context"
      ]
    }
  }
}
